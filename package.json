    "axios": "^0.15.3",
    "koa": "next",
    "koa-bodyparser": "next",
    "koa-route": "next",
    "latest-version": "^2.0.0",
    "mz": "^2.6.0",
    "package-json": "^2.4.0",
    "redis": "^2.6.3",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0",
    "string-hash": "^1.1.1",
    "targz": "^1.0.1",
    "typescript": "^2.1.6",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.37",
    "@types/mongoose": "^4.7.2",
    "@types/node": "^6.0.53",
    "@types/redis": "^0.12.34",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "jest": "^18.0.0",
